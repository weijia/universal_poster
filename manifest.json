{
  "name": "Universal poster",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "Post every stared thing to BAE",
  "permissions": [
    "https://api.twitter.com/*",
    "https://mycampus.duapp.com/*",
    //"*://reader.youdao.com/",
    "webRequest",
    "*://cang.baidu.com/"
  ],
  "icons": {
    "48" : "sample-48.png",
    "128" : "sample-128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js" : ["contentscript.js", "jquery-1.9.1.min.js"]
    }
  ],
  "options_page": "options.html",
  "background": {
    "scripts": ["poster.js", "cang_sniffer.js", "bae_backend.js", "background.js"]
  }
}
